{
  "name": "apiway-sdk-js",
  "version": "0.0.39",
  "license": "MIT",
  "description": "ApiWay API.",
  "main": "dist/components/ApiWay.js",
  "contributors": [],
  "readmeFilename": "README.md",
  "scripts": {
    "clean": "gulp clean",
    "build": "gulp build",
    "test": "mocha --opts ./mocha.opts test/*.spec.js",
    "test-coverage": "NODE_ENV=test nyc mocha --opts ./mocha.opts test/*.spec.js",
    "test-verbose": "DEBUG=apiway* npm test",
    "lint": "gulp lint",
    "make-docs": "node_modules/.bin/jsdoc -c .jsdoc.json --verbose",
    "release": "./release.sh 0.0.39",
    "codecov": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      [
        "add-module-exports",
        "transform-es2015-modules-umd"
      ]
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "axios": "^0.15.2",
    "debug": "^2.2.0",
    "js-base64": "^2.1.9",
    "lru-cache": "^4.1.1",
    "natives": "^1.1.0",
    "sigmund": "^1.0.1",
    "utf8": "^2.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-istanbul": "3.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0",
    "codecov": "^1.0.1",
    "del": "^2.2.0",
    "eslint-config-google": "^0.7.0",
    "eslint-plugin-mocha": "^4.7.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-eslint": "^3.0.1",
    "gulp-jscs": "^4.0.0",
    "gulp-jscs-stylish": "^1.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^2.2.0",
    "gulp-uglify": "^2.0.0",
    "jsdoc": "^3.4.0",
    "minami": "^1.1.1",
    "mocha": "^3.1.2",
    "must": "^0.13.1",
    "nock": "^9.0.2",
    "nyc": "9.0.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git://apiway.com/ApiWay/apiway.js.git"
  },
  "keywords": [
    "apiway",
    "api"
  ],
  "bugs": {
    "url": "https://apiway.com/ApiWay/apiway.js/issues"
  }
}
